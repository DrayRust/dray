{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Dray",
  "version": "1.0.0",
  "identifier": "dray",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "resources": [
      "ray/*"
    ],
    "active": true,
    "targets": "all",
    "icon": [
      "icons/64x64.png",
      "icons/icon.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
